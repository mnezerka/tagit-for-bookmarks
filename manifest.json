{
  "manifest_version": 3,
  "name": "TagIt for Bookmarks",
  "version": "1.0",
  "description": "Displays tagged (in brackets) bookmarks on the new tab page.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["bookmarks"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
